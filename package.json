  "scripts": {
    "start": "node build/server/index.js",
    "lint-full": "./scripts/lint-full.sh",
    "test": "./scripts/test.sh",
    "check": "npm run lint && npm run test && echo \"PASS!\"",
    "build:prod:client": "NODE_ENV=production webpack -p --progress",
